{
  "name": "medical-contract",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && pnpm run compact && tsc --project tsconfig.build.json && cp -r ./src/managed ./dist/managed && cp ./src/counter.compact ./dist",
    "compact": "compact compile src/counter.compact ./src/managed/counter",
    "test": "vitest run",
    "test:compile": "npm run compact && vitest run",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "setup": "npm run compact && npm run build"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.8.1"
  }
}
